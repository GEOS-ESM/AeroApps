      - {'$level': 500, '$region': 'sevseas', cmin: -50, cmax: 10, cint: 5, scale: log_scale}
      - {'$level': 700, '$region': 'sevseas', cmin: -35, cmax: 30, cint: 5}
      - {'$level': 850, '$region': 'sevseas', cmin: -35, cmax: 40, cint: 5}
