      - {'$level': 850, '$region': 'midatl', cmin: -35, cmax: 25, cint: 5}
      - {'$level': 10, '$region': 'australia', cmin: -80, cmax: -25, cint: 5}
      - {'$level': 30, '$region': 'australia', cmin: -75, cmax: -35, cint: 5}
      - {'$level': 50, '$region': 'australia', cmin: -70, cmax: -40, cint: 5}
      - {'$level': 100, '$region': 'australia', cmin: -95, cmax: -35, cint: 5}
