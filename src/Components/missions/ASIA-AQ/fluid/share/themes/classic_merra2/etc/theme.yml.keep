      - {'$level': 30, '$region': 'mideast', cmin: -70, cmax: -45, cint: 5}
      - {'$level': 50, '$region': 'mideast', cmin: -75, cmax: -50, cint: 5}
      - {'$level': 100, '$region': 'mideast', cmin: -85, cmax: -50, cint: 5}
